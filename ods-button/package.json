    "wct-mocha": "^1.0.0"
  },
  "watch": {
    "build-tokens": "./src/*.json"
  },
  "scripts": {
    "set-patch": "versiony ./src/package.json --patch",
    "copyfiles": "copyfiles -f ./node_modules/@alaskaair/orion-web-core-style-sheets/fonts/*.* ./demo/fonts/",
    "build-tokens": "node scripts/tokenScript.js",
    "build-tokens-custom": "node scripts/tokenScriptCustom.js",
    "sass-render-w": "sass-render -w src/**/*.{scss,css}",
    "sass-render": "sass-render src/**/*.{scss,css}",
    "build": "npm run sass-render | polymer serve",
    "dist": "npm run build-tokens-custom | npm run sass-render",
    "dev": "npm-watch build-tokens | npm-watch sass-render-w | polymer serve"
  }
}
