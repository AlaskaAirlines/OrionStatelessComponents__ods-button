    "@polymer/test-fixture": "^4.0.2",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "autoprefixer": "^9.4.7",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "css-byebye": "^2.0.0",
    "fs": "0.0.1-security",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
