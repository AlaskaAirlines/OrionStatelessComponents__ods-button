{
  "source": [ "./src/dancing-dots.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "./src/tokens/",
      "files": [
        {
          "destination": "_dotsVariables.scss",
          "format": "scss/variables"
        }
      ]
    },
    "css": {
      "transformGroup": "css",
      "buildPath": "./src/tokens/",
      "files": [
        {
          "destination": "dotsProperties.scss",
          "format": "css/variables"
        }
      ]
    }
  }
}
